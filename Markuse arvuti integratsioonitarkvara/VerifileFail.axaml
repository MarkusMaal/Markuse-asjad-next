<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="600" Height="320" WindowStartupLocation="CenterScreen" WindowState="FullScreen"
        x:Class="Markuse_arvuti_integratsioonitarkvara.VerifileFail"
        Title="Markuse asjad" Loaded="Window_Loaded_1">
  <StackPanel Margin="10" VerticalAlignment="Center">
    <Image Source="/Resources/failure.gif" Width="100"></Image>
    <Label FontSize="23" HorizontalContentAlignment="Center" Margin="0,20">Programmi laadimine nurjus</Label>
    <TextBlock Name="InfoTextBlock" TextWrapping="Wrap" TextAlignment="Center">
      Markuse asjad ei ole selles seadmes õigesti juurutatud. See programm ei tööta seadmetes, mis ei vasta Markuse asjade nõutele.
    </TextBlock>
    <Button HorizontalAlignment="Center" Margin="0,20,0,0" Click="Button_Click">Sulge programm</Button>
  </StackPanel>
</Window>
