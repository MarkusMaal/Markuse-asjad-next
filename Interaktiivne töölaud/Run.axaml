<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="420" Height="150" WindowStartupLocation="CenterOwner"
        x:Class="Interaktiivne_töölaud.Run" ShowInTaskbar="False"
        Title="Käivita" Foreground="White" Loaded="Window_Loaded">
  <Grid Margin="20">
    <Label>Sisestage käivitatav programm ja vajutage OK või Enter</Label>
    <Panel VerticalAlignment="Center">
      <TextBox Name="ProgName" KeyDown="TextBox_KeyDown"></TextBox>
    </Panel>
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom">
      <Button Margin="10,0" Width="75" Click="Cancel_Click">_Loobu</Button>
      <Button Width="75" Click="OK_Click">_OK</Button>
    </StackPanel>
  </Grid>
</Window>
